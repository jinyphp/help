{
    "name": "jiny/help",
    "description": "Jiny Help Package - Help Center, FAQ, Support, Guide functionality for Laravel applications",
    "type": "library",
    "keywords": ["laravel", "help", "faq", "support", "guide", "cms", "jiny"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jiny Team",
            "email": "contact@jiny.dev"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "jiny/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Help\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Help\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Help\\Providers\\JinyHelpServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.0"
}